{
  "name": "sandbox",
  "version": "0.0.0",
  
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "_package": "ng build --c=production --base-href=\"/PWA_DEMO_ENV_PUBLIC/\"",
    "_publish" : "npx angular-cli-ghpages --dir=dist/PWA_DEMO_ENV_PUBLIC",
    "package-and-publish": "git push && git pull && ng build --c=production --base-href=\"/PWA_DEMO_ENV_PUBLIC/\" && npx ngh --dir=dist\\PWA_DEMO_ENV_PUBLIC",
    "git-update": "git add . && git commit -m \"strategy update\" -a && git push && git pull"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.1",
    "@angular/common": "^17.0.1",
    "@angular/compiler": "^17.0.1",
    "@angular/core": "^17.0.1",
    "@angular/forms": "^17.0.1",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.1",
    "@angular/platform-browser-dynamic": "^17.0.1",
    "@angular/pwa": "^17.0.8",
    "@angular/router": "^17.0.1",
    "@angular/service-worker": "^17.0.1",
    "@eve-sama/ngx-signature-pad": "^16.0.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "mathjs": "^14.0.1",
    "rxjs": "~7.8.0",
    "socket.io": "^4.7.4",
    "three": "^0.172.0",
    "tslib": "^2.6.2",
    "tween": "^0.9.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "^17.0.1",
    "@angular/localize": "^17.0.1",
    "@types/jasmine": "~5.1.2",
    "@types/three": "^0.171.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  }
}
